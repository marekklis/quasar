{
  "name": "filter on date part",

  "backends": {
    "postgresql": "pending"
  },

  "data": "../days.data",

  "query": "select `day` from `../days` where date_part(\"dow\", ts) >= 3",

  "predicate": "containsExactly",

  "expected": [
    { "day": "Wednesday" },
    { "day": "Thursday" },
    { "day": "Friday" },
    { "day": "Saturday" }
  ]
}
